    <div class="col-lg-6 offset-lg-3 text-center" >
   
        <div class="form-group">
        <label for="description">Card Description, <a href="https://www.markdownguide.org/basic-syntax/" target="_blank">markdown</a> is supported.</label>
        <textarea class="form-control" name="description" id="description" style="width: 100%; height: 390px;" placeholder="Describe it, take your time..." oninput="updatedescription(this, `<%= data.name%>`)"><%= data.description%></textarea>
    
    </div>
    <div class="form-group">
      <label for="shortbio">Short Description:</label>
      <input class="form-control" name="shortbio" id="shortbio" placeholder="Make sure it's interesting!" value="<%= data.shortbio%>" oninput="updateshortbio(this, `<%= data.name%>`)">
    </div>
    <div class="form-group">
      <label for="location">Optional Location:</label>
      <input class="form-control" name="location" id="location" placeholder="Optional Location" value="<%= data.location%>" oninput="updatelocation(this, `<%= data.name%>`)">
    </div>

    </div>
<style>
